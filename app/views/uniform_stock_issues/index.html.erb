<h1>Listing uniform_stock_issues</h1>

<table>
  <thead>
    <tr>
      <th>Stock</th>
      <th>Issued to</th>
      <th>Issued by</th>
      <th>Issued on</th>
      <th>Quantity</th>
      <th>Document</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @uniform_stock_issues.each do |uniform_stock_issue| %>
      <tr>
        <td><%= uniform_stock_issue.stock_id %></td>
        <td><%= uniform_stock_issue.issued_to %></td>
        <td><%= uniform_stock_issue.issued_by %></td>
        <td><%= uniform_stock_issue.issued_on %></td>
        <td><%= uniform_stock_issue.quantity %></td>
        <td><%= uniform_stock_issue.document_id %></td>
        <td><%= link_to 'Show', uniform_stock_issue %></td>
        <td><%= link_to 'Edit', edit_uniform_stock_issue_path(uniform_stock_issue) %></td>
        <td><%= link_to 'Destroy', uniform_stock_issue, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Uniform stock issue', new_uniform_stock_issue_path %>
